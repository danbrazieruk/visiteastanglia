{# IG FEED
 # -- This is hacky. Were this a client, I'd definitely use the API!
 #}

{% set i = 1 %}

<div class="ig-feed d-flex flex-wrap">
    {% for i in range (1, 12) %}

        {# IG image #}
        <div class="ig-image bg-img" style="background-image: url('{{ siteUrl }}assets/img/instagram/{{ i }}.jpg');">
        </div>

        {% set i = i + 1 %}

    {% endfor %}
</div>